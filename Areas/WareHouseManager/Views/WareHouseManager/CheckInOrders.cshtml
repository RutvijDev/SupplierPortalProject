                            <tr>
                                <th style="width: 9%;">OrderID</th>
                                <th style="width: 10%;">Order Date</th>
                                <th style="width: 9%;">SupplierID</th>
                                <th style="width: 16%;">Product Name</th>
                                <th style="width: 14%;">Company Name</th>
                                <th style="width: 9%;">Quantity</th>
                                <th style="width: 9%;">TotalPrice</th>
                                <th style="width: 13%;">WareHouse Status</th>
                                <th style="width: 11%;">Delivery Status</th>
                            </tr>
                        </thead>
