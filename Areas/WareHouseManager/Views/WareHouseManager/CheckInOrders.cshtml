        <div class="row">
            <div class="col-12 text-center">
                <h2 class="text-white py-2">Checked-IN Orders</h2>
            </div>
        </div>
    </div>
    <div class="card-body">
        <div class="row pb-3">
            <div class="col-8">
            </div>
            <div class="col-4 text-end">
            </div>
        </div>
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-12">
                    <table id="tblData" class="table table-bordered table-striped custom-table" style="width:100%;">
