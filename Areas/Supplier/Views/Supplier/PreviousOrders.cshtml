@model IEnumerable<SupplierOrderVM>

<partial name="_Notification" />

<div class="card shadow border-0 my-3">
    <div class="card-header bg-secondary bg-gradient ml-0 py-3">
        <div class="row">
            <div class="col-12 text-center">
                <h2 class="text-white py-2">Previously Delivered Order</h2>
            </div>
        </div>
    </div>
    <div class="card-body">
        <div class="row pb-3">
            <div class="col-8">
            </div>
            <div class="col-4 text-end">
            </div>
        </div>
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-12">
                    <table id="tblData" class="table table-bordered table-striped custom-table" style="width:100%;">
                        <thead>
                            <tr>
                                <th style="width: 9%;">OrderID</th>
                                <th style="width: 15%;">Order Date</th>
                                <th style="width: 20%;">Product Name</th>
                                <th style="width: 13%;">Comapany</th>
                                <th style="width: 8%;">Quantity</th>
                                <th style="width: 9%;">TotalPrice</th>
                                <th style="width: 11%;">Order Status</th>
                                <th style="width: 15%;">Delivered Date</th>
                            </tr>
                        </thead>
@*                         <tbody>
                            @foreach (var obj in Model)
                            {
                                <tr>
                                    <td>@obj.SupplierOrder.OrderID</td>
                                    <td>@obj.Order.OrderDate</td>
                                    <td>@obj.Product.ProductName</td>
                                    <td>@obj.Product.CompanyName</td>
                                    <td>@obj.Order.Quantity</td>
                                    <td>₹ @obj.Order.TotalPrice</td>
                                    <td>@obj.SupplierOrder.Supplier_Order_Status</td>
                                    <td>@obj.SupplierOrder.Supplier_Delivered_Date</td>
                                </tr>
                            }
                        </tbody> *@
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/js/Supplier/PreviousOrders.js"></script>
}
